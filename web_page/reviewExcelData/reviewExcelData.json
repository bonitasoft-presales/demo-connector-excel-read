      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.context && $data.customers ){",
        "\treturn {",
        "\t\texcelFileDocumentInput: {",
        "\t\t\tid: $data.context.excelFile_ref && $data.context.excelFile_ref.id ? $data.context.excelFile_ref.id.toString() : null,",
        "\t\t\tfilename: $data.context.excelFile_ref.newValue && $data.context.excelFile_ref.newValue.filename ? $data.context.excelFile_ref.newValue.filename : null,",
        "\t\t\ttempPath: $data.context.excelFile_ref.newValue && $data.context.excelFile_ref.newValue.tempPath ? $data.context.excelFile_ref.newValue.tempPath : null,",
        "\t\t\tcontentType: $data.context.excelFile_ref.newValue && $data.context.excelFile_ref.newValue.contentType ? $data.context.excelFile_ref.newValue.contentType : null",
        "\t\t},",
        "\t\t//map customers variable to expected task contract input",
        "\t\tcustomersInput: $data.customers.map( it => ({",
        "\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\tname: it.name !== undefined ? it.name : null,",
        "\t\t\tdateOfBirth: it.dateOfBirth !== undefined ? it.dateOfBirth : null,",
        "\t\t\tincome: it.income !== undefined ? it.income : null",
        "\t\t}))",
        "\t}",
        "}"
